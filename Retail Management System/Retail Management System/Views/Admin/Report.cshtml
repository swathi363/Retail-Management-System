@model Retail_Management_System.Models.Transaction

@{
    ViewBag.Title = "Report";
}
@Html.Partial("_background")
@Html.Partial("admin_navbar")




<div class="container card m-5 bg-transparent">
    @using (Html.BeginForm("Report", "Admin", FormMethod.Post, new { @class = "form-inline m-3" }))
    {
        @Html.TextBox("TDate", null, new { @class = "form-control", @type = "Date" })

        <button type="submit" class="btn btn-success btn-sm ml-2">Generate Monthly Report</button>
    }
</div>


